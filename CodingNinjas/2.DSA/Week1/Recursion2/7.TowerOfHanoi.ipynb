    "def towerofhanoi(n, a, b, c):\n",
    "    # Please add your code here\n",
    "    if n <= 0:\n",
    "        return\n",
    "    towerofhanoi(n-1, a, c, b)\n",
    "    print(n,\"disk\",a, c)\n",
    "    towerofhanoi(n-1, b, a, c)\n",
    "\n",
    "\n",
    "n=int(input())\n",
    "towerofhanoi(n, 'a', 'b', 'c')"
