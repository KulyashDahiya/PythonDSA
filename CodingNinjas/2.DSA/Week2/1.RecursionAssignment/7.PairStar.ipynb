   "source": [
    "def pairStar(S):\n",
    "    if len(S) == 1:\n",
    "        return S[0]\n",
    "    \n",
    "    if S[0] == S[1]:\n",
    "        return S[0] + '*' + pairStar(S[1:])\n",
    "    else:\n",
    "        return S[0] + pairStar(S[1:])"
   ]
