   "source": [
    "def StringToInteger(S):\n",
    "    l = len(S)\n",
    "    if l == 0:\n",
    "        return 0\n",
    "\n",
    "    small = int(S[0]) * (10**(l-1))\n",
    "    lis = S[1:]\n",
    "    return small + StringToInteger(lis)\n",
    "\n",
    "\n",
    "S = input()\n",
    "\n",
    "print(StringToInteger(S))"
   ]
  }
 ],
