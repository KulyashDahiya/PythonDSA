   "source": [
    "def reversee(arr, i, j):\n",
    "    while i < j:\n",
    "        arr[i], arr[j] = arr[j], arr[i]\n",
    "        i += 1\n",
    "        j -= 1\n",
    "\n",
    "\n",
    "def rotate(arr, n, d):\n",
    "    #Your code goes here\n",
    "    reversee(arr, 0, n-1)\n",
    "    reversee(arr, 0, n-d-1)\n",
    "    reversee(arr, n-d, n-1)"
   ]
  }
