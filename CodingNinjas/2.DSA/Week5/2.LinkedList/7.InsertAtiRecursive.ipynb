   "source": [
    "def InsertAtI(head, i, data):\n",
    "    if i < 0:\n",
    "        return head\n",
    "    if i == 0:\n",
    "        newNode = Node(data)\n",
    "        newNode.next = head\n",
    "        return newNode\n",
    "    \n",
    "    if head is None:\n",
    "        return\n",
    "    \n",
    "    head.next = InsertAtI(head.next, i-1, data)\n",
    "\n",
    "    return head"
   ]
  }
