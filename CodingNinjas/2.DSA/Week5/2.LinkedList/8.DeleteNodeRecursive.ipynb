   "source": [
    "def deleteNodeRec(head, pos) :\n",
    "\t#Your code goes here\n",
    "    if head is None:\n",
    "        return\n",
    "\n",
    "    if pos < 0:\n",
    "        return head\n",
    "    \n",
    "    if pos == 0:\n",
    "        return head.next\n",
    "    \n",
    "    head.next = deleteNodeRec(head.next, pos-1)\n",
    "\n",
    "    return head"
   ]
  }
