   "source": [
    "def findNodeRec(head, n) :\n",
    "\t#Your code goes here\n",
    "    if head is None:\n",
    "        return -1\n",
    "\n",
    "    if head.data == n:\n",
    "        return 0\n",
    "\n",
    "    small_ans = findNodeRec(head.next, n)\n",
    "\n",
    "    if small_ans == -1:\n",
    "        return -1\n",
    "    \n",
    "    return 1 + small_ans"
   ]
  }
